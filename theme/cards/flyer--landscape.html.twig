<div class="flyer-landscape-wrapper">

    {% for item in attr(data, 'items') %}
        {% if acf(item, 'props.placement') == (placement ?? 99) %}
            {% set title = item.title %}
            
            <article class="flyer-landscape">
                <figure>
                    <a href="{{getPostHref(item.www)}}" title="{{title}}">
                        {{ image(title, item.image, 984, 200, 'r', 'img-thumb img-responsive', [0,0,0])|raw }}
                    </a>
                </figure>
            </article>

        {% endif %}
    {% endfor %}

</div>